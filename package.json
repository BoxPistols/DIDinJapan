{
  "name": "did-airspace-map",
  "version": "1.0.0",
  "description": "日本の人口集中地区（DID）・空港周辺空域・飛行禁止区域をインタラクティブに表示するドローン飛行計画ツール",
  "type": "module",
  "main": "./dist/lib/index.js",
  "module": "./dist/lib/index.js",
  "types": "./dist/lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/lib/index.js",
      "types": "./dist/lib/index.d.ts"
    },
    "./components": {
      "import": "./dist/components/index.js",
      "types": "./dist/components/index.d.ts"
    }
  },
  "files": [
    "dist/lib",
    "dist/components",
    "README.md"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:lib": "tsc --project tsconfig.lib.json",
    "preview": "vite preview",
    "test": "vitest",
    "prepublishOnly": "npm run build:lib",
    "fix": "npm run lint:fix && npm run format",
    "lint:fix": "eslint --fix --ext .ts,.tsx,.js,.jsx src",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepare": "husky",
    "download:facilities": "node scripts/download_osm_facilities.js",
    "docs:index": "node scripts/generate-docs-index.cjs"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.{json,md}": [
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BoxPistols/DID-J26.git"
  },
  "keywords": [
    "map",
    "japan",
    "gis",
    "geojson",
    "did",
    "population-density",
    "airspace",
    "restriction",
    "maplibre",
    "overlay",
    "airport",
    "weather",
    "無人機",
    "飛行禁止区域"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "maplibre-gl": "^4.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "dependencies": {
    "@mapbox/mapbox-gl-draw": "^1.5.1",
    "@turf/turf": "^7.3.2",
    "rbush": "^4.0.1",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-links": "^8.6.15",
    "@storybook/blocks": "^8.6.14",
    "@storybook/react": "^8.6.15",
    "@storybook/react-vite": "^8.6.15",
    "@storybook/test": "^8.6.15",
    "@types/geojson": "^7946.0.16",
    "@types/mapbox__mapbox-gl-draw": "^1.4.9",
    "@types/rbush": "^4.0.0",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.15",
    "husky": "^9.1.7",
    "jsdom": "^27.4.0",
    "lint-staged": "^16.2.7",
    "mapshaper": "^0.6.113",
    "node-notifier": "^10.0.1",
    "prettier": "^3.8.0",
    "storybook": "^8.6.15",
    "typescript": "^5.7.2",
    "vite": "^6.0.7",
    "vitest": "^4.0.17"
  }
}
